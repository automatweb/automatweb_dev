<?xml version='1.0'?>
<orb>
	<class name="ob_gen" >
		<action name="new">
			<function name="change">
				<arguments>
					<required name="parent"/>
					<optional name="alias_to"/>
					<optional name="return_url"/>
				</arguments>
			</function>
		</action>

		<action name="submit">
			<function name="submit">
				<arguments>
					<required name="id"/>
					<optional name="do"/>
				</arguments>
			</function>
		</action>

		<action name="generate_objects">
			<function name="generate_objects">
				<arguments>
					<required name="id"/>
				</arguments>
			</function>
		</action>

		<action name="change">
			<function name="change">
				<arguments>
					<optional name="parent"/>
					<required name="id"/>
					<optional name="return_url"/>
				</arguments>
			</function>
		</action>

		<action name="ob_conf">
			<function name="ob_conf">
				<arguments>
					<required name="id"/>
					<optional name="return_url"/>
				</arguments>
			</function>
		</action>
	</class>
</orb>
		
