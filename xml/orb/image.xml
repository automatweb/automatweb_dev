<?xml version='1.0'?>
<orb>
	<class name="image">
		<action name="new">
			<function name="add">
				<arguments>
					<required name="parent"/>
					<optional name="return_url"/>
					<optional name="alias_to"/>
				</arguments>
			</function>
		</action>

		<action name="submit">
			<function name="submit">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="change">
			<function name="change">
				<arguments>
					<required name="id"/>
					<optional name="return_url"/>
					<optional name="alias_to"/>
				</arguments>
			</function>
		</action>

		<action name="show" nologin="1">
			<function name="show">
				<arguments>
					<required name="file"/>
				</arguments>
			</function>
		</action>
	</class>
</orb>
		
