<?xml version='1.0'?>
<orb>
	<class name="mrp_resource" folder="mrp" extends="class_base">
		<action name="is_available">
			<function name="start_job">
				<arguments>
					<required name="resource" type="int" />
				</arguments>
			</function>
		</action>

		<action name="stop_job">
			<function name="stop_job">
				<arguments>
					<required name="resource" type="int" />
				</arguments>
			</function>
		</action>

	</class>
</orb>
