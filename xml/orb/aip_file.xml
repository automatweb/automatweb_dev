<?xml version='1.0'?>
<orb>
	<class name="aip_file" folder="www/dev/terryf/automatweb_dev/classes" extends="file">
		<action name="new" >
			<function name="add">
				<arguments>
					<optional name="parent" />
					<optional name="id" />
					<optional name="msg_id" />
					<optional name="return_url" />
					<optional name="alias_to" />
				</arguments>
			</function>
		</action>

		<action name="change" >
			<function name="change">
				<arguments>
					<required name="id" />
					<optional name="parent" />
					<optional name="return_url" />
					<optional name="doc" />
				</arguments>
			</function>
		</action>

		<action name="submit_change" >
			<function name="submit_change">
				<arguments>
					<required name="parent" />
					<required name="id" />
				</arguments>
			</function>
		</action>

		<action name="submit_add" >
			<function name="submit_add">
				<arguments>
					<required name="parent" />
					<optional name="id" />
				</arguments>
			</function>
		</action>

	</class>
</orb>
