<?xml version='1.0'?>
<orb>
	<class name="document" folder="" extends="aw_template">
		<action name="fetch">
			<function name="fetch">
				<arguments>
					<required name="docid" type="int" />
				</arguments>
			</function>
		</action>

		<action name="save">
			<function name="save">
				<arguments>
					<required name="id" acl="edit;view" />
				</arguments>
			</function>
		</action>

		<action name="new" is_public="1" caption="New document">
			<function name="add">
				<arguments>
					<required name="parent" acl="add" />
					<optional name="period" />
					<optional name="alias_to" />
					<optional name="return_url" />
					<optional name="reltype" />
				</arguments>
			</function>
		</action>

		<action name="change" is_public="1" caption="Edit document">
			<function name="change">
				<arguments>
					<required name="id" type="int" acl="edit;view" />
					<optional name="section" />
					<optional name="period" />
				</arguments>
			</function>
		</action>

		<action name="sellang">
			<function name="sel_lang_bros">
				<arguments>
					<required name="id" />
				</arguments>
			</function>
		</action>

		<action name="seb_s">
			<function name="seb_s">
				<arguments>
					<required name="id" />
				</arguments>
			</function>
		</action>

		<action name="set_lang_bro">
			<function name="set_lang_bro">
				<arguments>
					<required name="id" />
					<required name="sstring" />
					<required name="slang_id" />
					<required name="bro" />
				</arguments>
			</function>
		</action>

		<action name="sel_menus">
			<function name="sel_menus">
				<arguments>
					<required name="id" />
				</arguments>
			</function>
		</action>

		<action name="submit_menus">
			<function name="submit_menus">
				<arguments>
					<required name="id" />
				</arguments>
			</function>
		</action>

		<action name="search" nologin="1">
			<function name="do_search">
				<arguments>
					<optional name="parent" />
					<optional name="str" />
					<optional name="section" />
					<optional name="sortby" />
					<optional name="from" />
				</arguments>
			</function>
		</action>

		<action name="lookup" nologin="1">
			<function name="lookup">
				<arguments>
					<required name="id" />
					<optional name="sortby" />
					<optional name="origin" />
				</arguments>
			</function>
		</action>

		<action name="send" nologin="1">
			<function name="send">
				<arguments>
					<required name="section" />
				</arguments>
			</function>
		</action>

		<action name="submit_send" nologin="1">
			<function name="submit_send">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="feedback" nologin="1">
			<function name="feedback">
				<arguments>
					<required name="section" />
					<optional name="e" type="int" />
				</arguments>
			</function>
		</action>

		<action name="submit_feedback" nologin="1">
			<function name="submit_feedback">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="thanks" nologin="1">
			<function name="thanks">
				<arguments>
					<optional name="eesnimi" />
				</arguments>
			</function>
		</action>

		<action name="print" nologin="1">
			<function name="do_print">
				<arguments>
					<required name="section" />
				</arguments>
			</function>
		</action>

		<action name="docsearch" all_args="1">
			<function name="docsearch">
				<arguments>
				</arguments>
			</function>
		</action>

	</class>
</orb>
