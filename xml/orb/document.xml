<?xml version='1.0'?>
<orb>
	<class name="document">
		<action name="new">
			<function name="add">
				<arguments>
					<required name="parent"/>
					<required name="period"/>
				</arguments>
			</function>
		</action>
		<action name="change">
			<function name="change">
				<arguments>
					<required name="id"/>
				</arguments>
			</function>
		</action>
		<action name="submit_add">
			<function name="submit_add">
				<arguments>
					<required name="parent"/>
					<required name="period"/>
				</arguments>
			</function>
		</action>
		<action name="delete">
			<function name="delete">
				<arguments>
					<required name="parent"/>
					<required name="id"/>
				</arguments>
			</function>
		</action>
		<action name="save">
			<function name="save">
				<arguments>
					<required name="id"/>
				</arguments>
			</function>
		</action>
		<action name="preview">
			<function name="preview">
				<arguments>
					<required name="id"/>
				</arguments>
			</function>
		</action>
		<action name="sellang">
			<function name="sel_lang_bros">
				<arguments>
					<required name="id"/>
				</arguments>
			</function>
		</action>
		<action name="seb_s">
			<function name="seb_s">
				<arguments>
					<required name="id"/>
				</arguments>
			</function>
		</action>
		<action name="set_lang_bro">
			<function name="set_lang_bro">
				<arguments>
					<required name="id"/>
					<required name="sstring"/>
					<required name="slang_id"/>
					<required name="bro"/>
				</arguments>
			</function>
		</action>
		<action name="addalias">
			<function name="addalias">
				<arguments>
					<required name="id"/>
					<required name="alias"/>
				</arguments>
			</function>
		</action>
		<action name="sel_menus">
			<function name="sel_menus">
				<arguments>
					<required name="id"/>
				</arguments>
			</function>
		</action>
		<action name="submit_menus">
			<function name="submit_menus">
				<arguments>
					<required name="id"/>
				</arguments>
			</function>
		</action>
		<action name="delete_alias">
			<function name="del_alias">
				<arguments>
					<required name="id"/>
					<required name="docid"/>
				</arguments>
			</function>
		</action>
		<action name="add_bro">
			<function name="add_bro">
				<arguments>
					<required name="parent"/>
				</arguments>
			</function>
		</action>
		<action name="create_bro">
			<function name="create_bro">
				<arguments>
					<required name="parent"/>
					<required name="id"/>
					<optional name="s_name"/>
					<optional name="s_content"/>
				</arguments>
			</function>
		</action>
		<action name="archive">
			<function name="show_archive">
				<arguments>
					<required name="docid"/>
					<optional name="sortby"/>
				</arguments>
			</function>
		</action>
	</class>
		
</orb>
