<?xml version='1.0'?>
<orb>
	<class name="mail_message" extends="class_base" folder="messenger">
		<action name="deliver">
			<function name="deliver">
				<arguments>
					<required name="id" type="int"/>
				</arguments>
			</function>
		</action>

		<action name="show">
			<function name="show">
				<arguments>
					<required name="id" type="int"/>
				</arguments>
			</function>
		</action>
	</class>
</orb>
		
