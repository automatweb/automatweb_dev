<?xml version='1.0'?>
<orb>
	<class name="mail_message" folder="www/dev/terryf/awcl/classes/messenger" extends="class_base">
		<action name="deliver" >
			<function name="deliver">
				<arguments>
					<required name="id" type="int"/>
				</arguments>
			</function>
		</action>

		<action name="get_part" >
			<function name="get_part">
				<arguments>
					<required name="msgrid" type="int"/>
					<required name="msgid" type="int"/>
					<required name="mailbox" />
					<required name="part" type="int"/>
				</arguments>
			</function>
		</action>

	</class>
</orb>
