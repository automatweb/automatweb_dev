		<action name="show_pdf">
			<function name="show_pdf" nologin="1">
				<arguments>
					<required name="id" type="int" acl="view" />
				</arguments>
			</function>
		</action>
		<action name="show_print" nologin="1">
                        <function name="show_print">
                                <arguments>
                                        <required name="id" type="int" acl="view" />
                                </arguments>
                        </function>
                </action>
			
		<action name="show" nologin="1">
			<function name="show">
				<arguments>
					<required name="id" type="int" acl="view" />
				</arguments>
			</function>
		</action>

	</class>
</orb>
