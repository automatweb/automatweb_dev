<?xml version='1.0'?>
<orb>
	<class name="scheduler">
		<action name="new">
			<function name="add_sched">
				<arguments>
					<required name="parent" type="int"/>
				</arguments>
			</function>
		</action>

		<action name="change">
			<function name="change_sched">
				<arguments>
					<required name="id" type="int"/>
				</arguments>
			</function>
		</action>

		<action name="submit">
			<function name="submit">
				<arguments/>
			</function>
		</action>

		<action name="search_objs" all_args="1">
			<function name="search_objs">
				<arguments/>
			</function>
		</action>

		<action name="set_time">
			<function name="set_time">
				<arguments>
					<required name="id" type="int"/>
					<optional name="cycle"/>
				</arguments>
			</function>
		</action>

		<action name="update_repeaters">
			<function name="update_repeaters">
				<arguments>
					<required name="id"/>
				</arguments>
			</function>
		</action>

		<action name="do_events" nologin="1" default="1">
			<function name="do_events">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="show">
			<function name="show">
				<arguments>
					<optional name="sortby"/>
					<optional name="sort_order"/>
				</arguments>
			</function>
		</action>

		<action name="show_log">
			<function name="show_log">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="show_log_entry">
			<function name="show_log_entry">
				<arguments>
					<required name="id"/>
				</arguments>
			</function>
		</action>
	</class>
</orb>
		
