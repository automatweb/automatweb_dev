		</action>

		<action name="show">
			<function name="show">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="show_log">
			<function name="show_log">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="show_log_entry">
			<function name="show_log_entry">
