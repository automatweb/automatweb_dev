<?xml version='1.0'?>
<orb>
	<class name="scheduler" folder="" extends="aw_template">
		<action name="update_repeaters" >
			<function name="update_repeaters">
				<arguments>
					<required name="id" />
				</arguments>
			</function>
		</action>

		<action name="do_events" default="1" nologin="1">
			<function name="do_events">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="show" >
			<function name="show">
				<arguments>
					<optional name="sortby" />
					<optional name="sort_order" />
				</arguments>
			</function>
		</action>

		<action name="show_log" >
			<function name="show_log">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="show_log_entry" >
			<function name="show_log_entry">
				<arguments>
					<required name="id" />
				</arguments>
			</function>
		</action>

		<action name="new" >
			<function name="add_sched">
				<arguments>
					<required name="parent" type="int"/>
				</arguments>
			</function>
		</action>

		<action name="change" >
			<function name="change_sched">
				<arguments>
					<required name="id" type="int"/>
				</arguments>
			</function>
		</action>

		<action name="submit" >
			<function name="submit">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="search_objs" all_args="1">
			<function name="search_objs">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="set_time" >
			<function name="set_time">
				<arguments>
					<required name="id" type="int"/>
					<optional name="cycle" />
				</arguments>
			</function>
		</action>

		<action name="delete_repeater" >
			<function name="delete_repeater">
				<arguments>
					<required name="id" type="int"/>
					<required name="cycle" type="int"/>
				</arguments>
			</function>
		</action>

	</class>
</orb>
