<?xml version='1.0'?>
<orb>
	<class name="form_entry" folder="formgen">

		<action name="change">
			<function name="change">
				<arguments>
					<required name="id" acl="edit;view"/>
				</arguments>
			</function>
		</action>

		<action name="delete">
			<function name="delete">
				<arguments>
					<required name="id" acl="delete"/>
				</arguments>
			</function>
		</action>

	</class>
</orb>
		
