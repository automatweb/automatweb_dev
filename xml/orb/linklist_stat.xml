<?xml version='1.0'?>
<orb>
	<class name="linklist_stat">
		<action name="submit">
			<function name="submit">
				<arguments>
					<optional name="parent"/>
					<required name="id"/>
					<required name="sid"/>
				</arguments>
			</function>
		</action>

		<action name="change">
			<function name="change">
				<arguments>
					<optional name="parent"/>
					<optional name="id"/>
					<optional name="stat"/>
					<optional name="sid"/>
					<optional name="return_url"/>
				</arguments>
			</function>
		</action>

	</class>
</orb>
		
