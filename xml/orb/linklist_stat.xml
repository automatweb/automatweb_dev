<?xml version='1.0'?>
<orb>
	<class name="linklist_stat" folder="" extends="aw_template">
		<action name="change" >
			<function name="change">
				<arguments>
					<optional name="parent" />
					<optional name="id" />
					<optional name="stat" />
					<optional name="sid" />
					<optional name="return_url" />
				</arguments>
			</function>
		</action>

		<action name="submit" >
			<function name="submit">
				<arguments>
					<required name="id" />
					<required name="sid" />
					<optional name="parent" />
				</arguments>
			</function>
		</action>

	</class>
</orb>
