<?xml version='1.0'?>
<orb>
	<class name="realestate_property" folder="applications/realestate_management" extends="class_base">
		<action name="set_customer">
			<function name="set_customer">
				<arguments>
					<required name="id" type="int" />
					<required name="client_oid" type="int" />
					<required name="client_type" />
					<optional name="close" />
				</arguments>
			</function>
		</action>

		<action name="customer_search" all_args="1">
			<function name="customer_search">
				<arguments>
					<required name="id" type="int" />
					<required name="client_type" />
					<required name="manager" type="int" />
				</arguments>
			</function>
		</action>

		<action name="pictures_view" nologin="1">
			<function name="pictures_view">
				<arguments>
					<required name="id" type="int" />
				</arguments>
			</function>
		</action>

		<action name="print" nologin="1">
			<function name="print_view">
				<arguments>
					<required name="id" type="int" />
					<required name="contact_type" />
					<optional name="show_pictures" />
					<optional name="view_type" />
					<optional name="return_url" />
				</arguments>
			</function>
		</action>

		<action name="save_map_data" nologin="1">
			<function name="save_map_data">
				<arguments>
					<required name="id" type="int" />
					<optional name="mapUrl" />
					<optional name="mapPoint" />
					<optional name="mapArea" />
					<optional name="mapId" />
				</arguments>
			</function>
		</action>

	</class>
</orb>
