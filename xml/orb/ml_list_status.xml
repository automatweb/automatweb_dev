<?xml version='1.0'?>
<orb>
	<class name="ml_list_status" folder="mailinglist" extends="ml_queue">
		<action name="new">
			<function name="add">
				<arguments>
					<required name="parent" acl="add"/>
					<optional name="alias_to"/>
					<optional name="return_url"/>
				</arguments>
			</function>
		</action>

		<action name="submit">
			<function name="submit">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="change">
			<function name="change">
				<arguments>
					<required name="id" acl="view;change"/>
					<optional name="return_url"/>
				</arguments>
			</function>
		</action>
	</class>
</orb>
		
