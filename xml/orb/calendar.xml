<?xml version='1.0'?>
<orb>
	<class name="calendar">
		<action name="new">
			<function name="add">
				<arguments>
					<required name="parent" type="int"/>
					<optional name="alias_to"/>
					<optional name="return_url"/>
				</arguments>
			</function>
		</action>

		<action name="change">
			<function name="change">
				<arguments>
					<required name="id" type="int"/>
					<optional name="return_url"/>
				</arguments>
			</function>
		</action>

		<action name="submit">
			<function name="submit">
				<arguments/>
			</function>
		</action>

		<action name="search_document" all_args="1">
			<function name="search_document">
				<arguments>
					<required name="id" type="int"/>
					<optional name="return_url"/>
				</arguments>
			</function>
		</action>

		<action name="pick_document">
			<function name="pick_document">
				<arguments>
					<required name="id" type="int"/>
					<required name="docid" type="int"/>
					<required name="return_url"/>
				</arguments>
			</function>
		</action>

		<action name="month">
			<function name="gen_month">
				<arguments>
					<optional name="date"/>
				</arguments>
			</function>
		</action>
	</class>
</orb>
		
