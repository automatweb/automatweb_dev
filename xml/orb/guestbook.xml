<?xml version='1.0'?>
<orb>
	<class name="guestbook" folder="" extends="aw_template">
		<action name="new" >
			<function name="add">
				<arguments>
					<required name="parent" acl="add"/>
					<optional name="docid" />
				</arguments>
			</function>
		</action>

		<action name="submit_gb" >
			<function name="submit_gb">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="change" >
			<function name="change">
				<arguments>
					<required name="id" acl="edit;view"/>
					<optional name="parent" />
					<optional name="docid" />
				</arguments>
			</function>
		</action>

		<action name="add_entry" >
			<function name="add_entry">
				<arguments>
					<required name="id" />
					<required name="url" />
				</arguments>
			</function>
		</action>

	</class>
</orb>
