<?xml version='1.0'?>
<orb>

	<class name="aliasmgr">
	
		<action name="list_aliases">
			<function name="new_list_aliases">
				<arguments>
					<required name="id" type="int"/>
					<optional name="table"/>
					<optional name="sortby"/>
					<optional name="sort_order"/>
				</arguments>
			</function>
		</action>

		<action name="submit_list">
			<function name="submit_list">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="search" all_args="1">
			<function name="search">
				<arguments/>
			</function>
		</action>
		
		<action name="addalias">
			<function name="addalias">
				<arguments>
					<required name="id"/>
					<required name="alias"/>
					<optional name="_cl"/>
					<optional name="_ac"/>
				</arguments>
			</function>
		</action>

	</class>
</orb>
