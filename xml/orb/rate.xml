<?xml version='1.0'?>
<orb>
	<class name="rate" folder="contentmgmt/rate" extends="class_base">

		<action name="rate" nologin="1">
			<function name="add_rate">
				<arguments>
					<required name="oid" type="int"/>
					<required name="return_url"/>
					<required name="rate"/>
				</arguments>
			</function>
		</action>

		<action name="show" nologin="1">
			<function name="show">
				<arguments>
					<required name="id" type="int"/>
					<optional name="gallery_id" type="int"/>
				</arguments>
			</function>
		</action>
		

	</class>
</orb>
		
