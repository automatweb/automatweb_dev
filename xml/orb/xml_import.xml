<?xml version='1.0'?>
<orb>
	<class name="xml_import">

		<action name="new">
			<function name="change">
				<required name="parent" type="int"/>
			</function>
		</action>

		<action name="change">
			<function name="change">
				<required name="id" type="int"/>
			</function>
		</action>

		<action name="submit">
			<function name="submit"/>
		</action>

		<action name="invoke">
			<function name="invoke">
				<required name="id"/>
			</function>
		</action>

		<action name="import_tudengid" nologin="1">
			<function name="import_tudengid"/>
		</action>
		
		<action name="import_oppekava" nologin="1">
			<function name="import_oppekava"/>
		</action>
		
		<action name="import_struktuurid" nologin="1">
			<function name="import_struktuurid"/>
		</action>
		
		<action name="import_tootajad" nologin="1">
			<function name="import_tootajad"/>
		</action>
		
	</class>
</orb>
		
