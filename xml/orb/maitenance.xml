<?xml version='1.0'?>
<orb>
	<class name="maitenance"  extends="class_base">
		<action name="cache_clear">
			<function name="cache_clear">
				<arguments>
					<optional name="clear"/>
					<optional name="list"/>
				</arguments>
			</function>
		</action>
		<action name="kb_convert">
			<function name="kb_convert">
				<arguments>
					<optional name="next"/>
					<optional name="samm"/>
					<optional name="convert"/>
					<optional name="parent"/>
				</arguments>
			</function>
		</action>
		<action name="ta_convert">
			<function name="ta_convert">
				<arguments>
					<optional name="next"/>
					<optional name="samm"/>
				</arguments>
			</function>
		</action>
	</class>
</orb>
