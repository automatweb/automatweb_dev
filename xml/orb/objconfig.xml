<?xml version='1.0'?>
<orb>
	<class name="objconfig">
		<action name="new">
			<function name="add">
				<arguments>
					<required name="parent" type="int"/>
				</arguments>
			</function>
		</action>
		
		<action name="change">
			<function name="change">
				<argument>
					<required name="id" type="int"/>
				</argument>
			</function>
		</action>
		
		<action name="submit_add">
			<function name="submit_add">
				<arguments/>
			</function>
		</action>
		
		<action name="submit">
			<function name="submit">
				<arguments/>
			</function>
		</action>
		
	</class>
</orb>
