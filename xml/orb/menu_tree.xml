<?xml version='1.0'?>
<orb>
	<class name="menu_tree">

		<action name="new">
			<function name="change">
				<arguments>
					<required name="parent"/>
					<optional name="alias_to"/>
					<optional name="return_url"/>
				</arguments>
			</function>
		</action>

		<action name="change">
			<function name="change">
				<arguments>
					<required name="id"/>
					<optional name="alias_to"/>
					<optional name="return_url"/>
				</arguments>
			</function>
		</action>

		<action name="submit">
			<function name="submit"/>
		</action>

	</class>
</orb>
		
