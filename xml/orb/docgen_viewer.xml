<?xml version='1.0'?>
<orb>
	<class name="docgen_viewer" folder="core/docgen" extends="class_base">
		<action name="intro">
			<function name="intro">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="class_list">
			<function name="class_list">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="frames" default="1">
			<function name="frameset">
				<arguments>
					<optional name="id" type="int" />
				</arguments>
			</function>
		</action>

		<action name="prop_info">
			<function name="prop_info">
				<arguments>
					<required name="option" />
					<required name="id" />
				</arguments>
			</function>
		</action>

		<action name="class_info" caption="N&amp;auml;ita klassi infot">
			<function name="class_info">
				<arguments>
					<required name="file" />
					<optional name="api_only" />
				</arguments>
			</function>
		</action>

		<action name="search_method">
			<function name="search_method">
				<arguments>
					<required name="method" />
				</arguments>
			</function>
		</action>

		<action name="view_source">
			<function name="view_source">
				<arguments>
					<required name="file" />
					<required name="v_class" />
					<optional name="func" />
				</arguments>
			</function>
		</action>

		<action name="doclist">
			<function name="doclist">
				<arguments>
					<optional name="type" default="classes" />
				</arguments>
			</function>
		</action>

		<action name="show_doc">
			<function name="show_doc">
				<arguments>
					<required name="file" />
				</arguments>
			</function>
		</action>

		<action name="do_db_update">
			<function name="do_db_update">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="view_usage">
			<function name="view_usage">
				<arguments>
					<required name="file" />
					<required name="v_class" />
					<required name="func" />
				</arguments>
			</function>
		</action>

		<action name="topf">
			<function name="topf">
				<arguments>
					<optional name="id" />
				</arguments>
			</function>
		</action>

		<action name="proplist">
			<function name="doc_proplist">
				<arguments>
					<required name="id" />
				</arguments>
			</function>
		</action>

		<action name="doc_class_list">
			<function name="doc_class_list">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="api_class_list">
			<function name="api_class_list">
				<arguments>
				</arguments>
			</function>
		</action>

	</class>
</orb>
