<?xml version='1.0'?>
<orb>
	<class name="community" folder="applications/commune" extends="class_base">
		<action name="add_blocked" all_args="1">
			<function name="add_blocked">
				<arguments>
					<required name="sel" type="int" acl="view" />
					<required name="id" type="int" acl="view" />
					<optional name="group" />
				</arguments>
			</function>
		</action>

		<action name="add_moderator" all_args="1">
			<function name="add_moderator">
				<arguments>
					<required name="sel" type="int" acl="view" />
