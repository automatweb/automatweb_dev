		</action>

		<action name="add_member" all_args="1">
			<function name="add_member">
				<arguments>
					<required name="sel" type="int" acl="view" />
					<required name="id" type="int" acl="view" />
					<optional name="group" />
				</arguments>
			</function>
		</action>

		<action name="remove_con" all_args="1">
