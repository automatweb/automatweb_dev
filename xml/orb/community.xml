				<arguments>
					<required name="sel" type="int" acl="view" />
					<required name="id" type="int" acl="view" />
					<optional name="group" />
				</arguments>
			</function>
		</action>

		<action name="change" nologin="1" is_public="1" all_args="1" caption="Kogukonna sisu">
			<function name="change">
				<arguments>
					<required name="id" type="int" acl="view" />
					<optional name="group" />
				</arguments>
			</function>
		</action>

	</class>
</orb>
