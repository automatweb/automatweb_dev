			<function name="add">
				<arguments>
					<required name="parent" acl="add" />
					<optional name="alias_to" />
					<optional name="return_url" />
				</arguments>
			</function>
		</action>

		<action name="submit">
			<function name="submit">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="set_time">
			<function name="change">
				<arguments>
					<required name="id" type="int" />
