<?xml version='1.0'?>
<orb>
	<class name="repeater_obj" extends="cal_event">
		<action name="new">
			<function name="add">
				<arguments>
					<required name="parent"/>
					<optional name="alias_to"/>
					<optional name="return_url"/>
				</arguments>
			</function>
		</action>

		<action name="submit">
			<function name="submit">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="change">
			<function name="change">
				<arguments>
					<required name="id"/>
					<optional name="return_url"/>
					<optional name="cycle"/>
				</arguments>
			</function>
		</action>

		<action name="set_time">
			<function name="change">
				<arguments>
					<required name="id" type="int"/>
					<optional name="cycle"/>
				</arguments>
			</function>
		</action>
	</class>
</orb>
		
