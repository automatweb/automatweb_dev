<?xml version='1.0'?>
<orb>
	<class name="treeview" folder="vcl">
		<action name="new">
			<function name="add">
				<arguments>
					<required name="parent"/>
				</arguments>
			</function>
		</action>

		<action name="change">
			<function name="change">
				<arguments>
					<required name="id"/>
				</arguments>
			</function>
		</action>

		<action name="show">
			<function name="show">
				<arguments>
					<required name="id"/>
				</arguments>
			</function>
		</action>
	</class>
</orb>
		
