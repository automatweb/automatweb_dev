<?xml version='1.0'?>
<orb>
	<class name="contact" folder="kliendibaas" extends="class_base">
		<action name="show">
			<function name="show">
				<arguments>
					<required name="id"/>
				</arguments>
			</function>
		</action>

		<action name="pop_select">
			<function name="pop_select">
				<arguments>
					<required name="tyyp"/>
