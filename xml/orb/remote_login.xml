<?xml version='1.0'?>
<orb>
	<class name="remote_login">
		<action name="new">
			<function name="change">
				<arguments>
					<required name="parent" type="int"/>
				</arguments>
			</function>
		</action>
		
		<action name="change">
			<function name="change">
				<arguments>
					<required name="id" type="int"/>
				</arguments>
			</function>
		</action>
		
		<action name="submit">
			<function name="submit"/>
		</action>

		<action name="getcookie" public="1" nologin="1">
			<function name="getcookie"/>
		</action>
	</class>
</orb>
		
