<?xml version='1.0'?>
<orb>
	<class name="messenger_v2" folder="messenger" extends="class_base">
		<action name="get_part">
			<function name="get_part">
				<arguments>
					<required name="id" type="int"/>
					<required name="msgid" type="int"/>
					<required name="mailbox"/>
					<required name="part" type="int"/>
				</arguments>		
			</function>
		</action>

		<action name="run_filters" nologin="1">
			<function name="run_filters">
				<arguments>
					<required name="id" type="int"/>
				</arguments>
			</function>
		</action>
		
		<action name="test_filters">
			<function name="test_filters">
				<arguments>
					<required name="id" type="int"/>
				</arguments>
			</function>
		</action>
	</class>
</orb>
		
