<?xml version='1.0'?>
<orb>
	<class name="linklist">
		<action name="new">
			<function name="add">
				<arguments>
					<required name="parent"/>
					<optional name="alias_to"/>
					<optional name="return_url"/>
				</arguments>
			</function>
		</action>

		<action name="show">
			<function name="show">
				<arguments>
					<optional name="id"/>
					<optional name="aktiivne"/>
					<optional name="otsi"/>
				</arguments>
			</function>
		</action>

		<action name="submit">
			<function name="submit">
				<arguments>
				</arguments>
			</function>
		</action>

		<action name="change">
			<function name="change">
				<arguments>
					<required name="id"/>
					<optional name="return_url"/>
				</arguments>
			</function>
		</action>
	</class>
</orb>
		
