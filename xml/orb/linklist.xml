<?xml version='1.0'?>
<orb>
	<class name="linklist" folder="" extends="class_base">
		<action name="show" >
			<function name="show">
				<arguments>
					<optional name="id" />
					<optional name="cd" />
					<optional name="search" />
				</arguments>
			</function>
		</action>

		<action name="goto" >
			<function name="link_redirect">
				<arguments>
					<optional name="id" />
					<optional name="lkid" />
				</arguments>
			</function>
		</action>

	</class>
</orb>
