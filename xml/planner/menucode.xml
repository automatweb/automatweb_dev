<?xml version='1.0'?>
<planner>
	<config>
		<caption>Kalendri konf</caption>
		<link>?class=planner&amp;action=change&amp;id={VAR:id}</link>
	</config>
	<today>
		<caption>Täna</caption>
		<link>?class=planner&amp;action=view&amp;id={VAR:id}</link>
	</today>
	<overview>
		<caption>Ülevaade</caption>
		<link>?class=planner&amp;action=view&amp;id={VAR:id}&amp;date={VAR:date}&amp;type=overview</link>
	</overview>
	<day>
		<caption>Päev</caption>
		<link>?class=planner&amp;action=view&amp;id={VAR:id}&amp;date={VAR:date}&amp;type=day</link>
	</day>
	<week>
		<caption>Nädal</caption>
		<link>?class=planner&amp;action=view&amp;id={VAR:id}&amp;date={VAR:date}&amp;type=week</link>
	</week>
	<month>
		<caption>Kuu</caption>
		<link>?class=planner&amp;action=view&amp;id={VAR:id}&amp;date={VAR:date}&amp;type=month</link>
	</month>
</planner>
