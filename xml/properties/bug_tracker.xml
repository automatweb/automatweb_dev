<?xml version='1.0'?>
<properties>
      <property>
            <name>bug_tb</name>
            <table>objects</table>
            <group>bugs</group>
            <type>toolbar</type>
            <no_caption>1</no_caption>
            <store>no</store>
            <field>bug_tb</field>
      </property>
      <property>
            <name>bug_tree</name>
            <table>objects</table>
            <group>bugs</group>
            <type>treeview</type>
            <parent>bug</parent>
            <no_caption>1</no_caption>
            <store>no</store>
            <field>bug_tree</field>
      </property>
      <property>
            <name>bug_list</name>
            <table>objects</table>
            <group>bugs</group>
            <type>table</type>
            <parent>bug</parent>
            <no_caption>1</no_caption>
            <store>no</store>
            <field>bug_list</field>
      </property>
      <classinfo>
            <syslog_type>ST_BUG_TRACKER</syslog_type>
            <relationmgr>yes</relationmgr>
            <no_comment>1</no_comment>
            <no_status>1</no_status>
      </classinfo>
      <groupinfo>
            <bugs>
                  <caption>Bugid</caption>
                  <submit>no</submit>
            </bugs>
      </groupinfo>
      <layout>
            <bug>
                  <type>hbox</type>
                  <width>15%:85%</width>
                  <group>bugs</group>
            </bug>
      </layout>
</properties>

