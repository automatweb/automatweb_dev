			obj { class_id=CL_MENU, parent=${admin_root}, name="Programmide ACL" type=MN_ADMIN1, admin_feature=PRG_ACCESSMGR, status=STAT_ACTIVE }
			obj { class_id=CL_MENU, parent=${admin_root}, name="Config" type=MN_ADMIN1, admin_feature=PRG_CONFIG, status=STAT_ACTIVE }
				

