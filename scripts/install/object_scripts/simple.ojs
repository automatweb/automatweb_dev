# menyyd on root/klient/$url
$root = obj{ class_id=CL_MENU, parent=${parent}, name="Klient", type=MN_CLIENT, status=STAT_ACTIVE}
	$klient = obj { class_id=CL_MENU, parent=${root}, name=${url}, type=MN_CLIENT, status=STAT_ACTIVE }
		$site_root = obj { class_id=CL_MENU, parent=${klient}, jrk="2", name=${url}, type=MN_CLIENT, status=STAT_ACTIVE }


			# login menyy
