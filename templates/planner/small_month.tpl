<table border="0" cellspacing="0" cellpadding="1">
<tr><td colspan="7" class="celltext">
{VAR:caption}
</td></tr>
<tr>
<td class="caldayname">E</td>
<td class="caldayname">T</td>
<td class="caldayname">K</td>
<td class="caldayname">N</td>
<td class="caldayname">R</td>
<td class="caldayname"><font color="red">L</font></td>
<td class="caldayname"><font color="red">P</font></td>
</tr>

<!-- SUB: week -->
<tr>

<!-- SUB: empty -->
<td class="calday" valign="middle">&nbsp;</td>
<!-- END SUB: empty -->

<!-- SUB: cell -->
