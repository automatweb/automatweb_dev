<!-- END SUB: cell -->

<!-- SUB: activecell -->
<td class="caltoday" align="center" valign="middle"><a href="{VAR:dayorblink}">{VAR:nday}</a></td>
<!-- END SUB: activecell -->

</tr>
<!-- END SUB: week -->


</table>
